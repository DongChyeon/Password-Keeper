<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dongchyeon.passwordkeeper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_logo"
        android:label="@string/app_name"
        android:name=".App"
        android:roundIcon="@mipmap/ic_app_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".presentation.view.MainActivity" />
        <activity android:name=".presentation.view.MemoDetailActivity" />
        <activity android:name=".presentation.view.MemoEditActivity" />
        <activity android:name=".presentation.view.RegisterActivity" />
        <activity
            android:name=".presentation.view.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.view.MemoListActivity" />
    </application>
</manifest>